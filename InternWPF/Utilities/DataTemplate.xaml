<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:InternWPF.ViewModel"
                    xmlns:view="clr-namespace:InternWPF.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UsersVM}">
        <view:Users />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Settings />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:JournalsVM}">
        <view:Journals />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:EntriesVM}">
        <view:Entries />
    </DataTemplate>

</ResourceDictionary>